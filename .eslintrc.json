{
  "plugins": ["babel", "react"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "env": {
    "browser": true,
    "es6": true,
    "mocha": true,
    "jest": true
  },
  "globals": {
    "Backbone": true,
    "jest": true,
    "define": true,
    "document": true,
    "dust": true,
    "Foundation": true,
    "Marionette": true,
    "Modernizr": true,
    "module": true,
    "require": true,
    "requirejs": true,
    "after": false,
    "afterEach": false,
    "assert": false,
    "before": false,
    "beforeEach": false,
    "describe": false,
    "expect": false,
    "it": false,
    "mocha": false,
    "sinon": false
  },
  "rules": {
    "array-bracket-spacing": 1,
    "arrow-parens": 1,
    "arrow-spacing": 1,
    "brace-style": [1, "1tbs", {"allowSingleLine": true}],
    "comma-spacing": [1, {"after": true}],
    "curly": 1,
    "dot-notation": [1, {"allowKeywords": true}],
    "eol-last": 1,
    "eqeqeq": 1,
    "generator-star-spacing": 1,
    "guard-for-in": 1,
    "key-spacing": [1, {"mode": "strict"}],
    "keyword-spacing": [1, {}],
    "max-len": [1, 150, {"ignoreComments": true}, {"ignoreTrailingComments": true}],
    "new-parens": 1,
    "no-bitwise": 1,
    "no-caller": 1,
    "no-cond-assign": 1,
    "no-const-assign": 2,
    "no-console": [1, {"allow": ["warn", "error"] }],
    "no-debugger": 1,
    "no-dupe-args": 1,
    "no-dupe-keys": 1,
    "no-duplicate-case": 1,
    "no-empty": 1,
    "no-eval": 1,
    "no-extend-native": 1,
    "no-extra-semi": 1,
    "no-fallthrough": 1,
    "no-irregular-whitespace": 1,
    "no-iterator": 1,
    "no-loop-func": 1,
    "no-mixed-spaces-and-tabs": 1,
    "no-multi-spaces": 1,
    "no-multi-str": 1,
    "no-multiple-empty-lines": [1, {"max": 1}],
    "no-obj-calls": 1,
    "no-process-exit": 1,
    "no-proto": 1,
    "no-return-assign": 1,
    "no-script-url": 1,
    "no-spaced-func": 1,
    "no-this-before-super": 1,
    "no-trailing-spaces": 1,
    "no-undef": 1,
    "no-unexpected-multiline": 1,
    "no-unreachable": 1,
    "no-unused-vars": 1,
    "no-use-before-define": 1,
    "no-with": 1,
    "object-curly-spacing": 1,
    "object-shorthand": 1,
    "operator-linebreak": [1, "after"],
    "quotes": [1, "single", {"allowTemplateLiterals": true}],
    "jsx-quotes": ["error", "prefer-single"],
    "react/jsx-closing-bracket-location": 1,
    "react/jsx-equals-spacing": 1,
    "react/jsx-indent": [1, "space"],
    "react/jsx-indent-props": [1, "space"],
    "react/jsx-no-duplicate-props": 1,
    "react/jsx-no-undef": 1,
    "react/jsx-tag-spacing": [1, {"beforeSelfClosing": "always"}],
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 1,
    "react/no-unescaped-entities": 1,
    "react/no-unknown-property": 1,
    "require-yield": 1,
    "semi": [0, "never"],
    "space-before-blocks": [1, "always"],
    "space-before-function-paren": [1, "never"],
    "space-in-parens": [1, "never"],
    "space-infix-ops": 1,
    "space-unary-ops": [1, {"nonwords": false, "overrides": {}}],
    "spaced-comment": [1, "always", {"exceptions": ["*"],"markers": ["global"]}],
    "valid-typeof": 1,
    "wrap-iife": [1, "any"],
    "yield-star-spacing": [1, "after"]
  }
}
